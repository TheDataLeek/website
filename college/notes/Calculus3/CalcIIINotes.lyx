\begin_layout Subsection

Lines
\end_layout

\begin_layout Standard

Building a line in 
\begin_inset Formula $\mathbb{R}^3$
\end_inset

: 
\end_layout

\begin_layout Enumerate

Find a point 
\begin_inset Formula $(x_0,y_0,z_0)$
\end_inset

 
\end_layout

\begin_layout Enumerate

Find a vector 
\begin_inset Formula $\vec{v}=(v_1,v_2,v_3)$
\end_inset

 
\end_layout

\begin_layout Enumerate

Vector between 
\begin_inset Formula $(x(t),y(t),z(t))$
\end_inset

 and 
\begin_inset Formula $(x_0,y_0,z_0)$
\end_inset

 is 
\begin_inset Formula $\langle x(t)-x_0,y(t)-y_0,z(t)-z_0\rangle = t\nvec{v_1,v_2,v_3}$
\end_inset

 
\end_layout

\begin_layout Enumerate


\begin_inset Formula \begin{equation}
    \boxed{\text{Equation of a Line:}\\
        \begin{cases}
            x(t)=x_0+tv_1\\
            y(t)=y_0+tv_2\\
            z(t)=z_0+tv_3
        \end{cases}}
    \end{equation}
\end_inset


\end_layout

\begin_layout Subsubsection

Example - Building a Line in 
\begin_inset Formula $\mathbb{R}^3$
\end_inset


\end_layout

\begin_layout Standard

Given point 
\begin_inset Formula $P_0(-2,0,4)$
\end_inset

 and vector 
\begin_inset Formula $\vec{v}=\nvec{2,4,-2}$
\end_inset


\begin_inset Newline newline
\end_inset

 
\begin_inset Formula \[
            \begin{cases}
                x(t)=-2+2t\\
                y(t)=0+4t\\
                z(t)=4-2t
            \end{cases}
        \]
\end_inset


\end_layout

\begin_layout Subsubsection

Example - Building a Line in 
\begin_inset Formula $\mathbb{R}^3$
\end_inset


\end_layout

\begin_layout Standard

Find an equation for the line between 
\begin_inset Formula $P(-3,2,-3)$
\end_inset

 and the point 
\begin_inset Formula $Q(1,-1,4)$
\end_inset


\begin_inset Newline newline
\end_inset

 
\begin_inset Formula \[
        \begin{aligned}
            \vec{PQ}=\nvec{4,-3,7}\\
            \begin{cases}
                x(t)=-3+4t\\
                y(t)=2-3t\\
                z(t)=-3-7t
            \end{cases}
        \end{aligned}
        \]
\end_inset


\end_layout

\begin_layout Subsubsection

Example - Distance from a Point to a Line
\end_layout

\begin_layout Standard

Distance from a point 
\begin_inset Formula $s$
\end_inset

 to a line 
\begin_inset Formula $l$
\end_inset

 
\begin_inset Formula \[
        \begin{aligned}
        d=|\vec{PS}|\sin(\theta)\\
        |\vec{PS}\times\vec{v}|=|\vec{PS}||\vec{v}|\sin(\theta)\\
        |\vec{PS}|\sin(\theta)=\frac{|\vec{SP}\times\vec{v}|}{|\vec{v}|}\\
        \text{Distance from }s\text{ to }l\equiv d=\frac{|\vec{SP}\times\vec{v}|}{|\vec{v}|}
        \end{aligned}
        \]
\end_inset


\end_layout

